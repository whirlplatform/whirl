        <event id="EDED446A-C1BD-4CC1-BBDA-8A54E8415BCA" name="Click event (app scope)" type="JavaScript">
            <properties>
                <schema></schema>
                <code>evt_click_global</code>
                <function></function>
                <source><![CDATA[//function
${function}(ctx){
  console.log(ctx); //Whirl.JavaScriptContext
  var param = ctx.getParameter(0);
  var type = param.getType();
  console.log('type: ' + type);
  var value = null;
  if(type == 'STRING'){
  	value = param.getStringValue();
    var builder = Whirl.Builders.findBuilderByCode('tf_component');
    builder.setValue(value);
  } else if(type == 'NUMBER'){
    value = param.getNumberValue();
    var builder = Whirl.Builders.findBuilderByCode('nf_component');
    builder.setValue(value);
  } else if(type == 'BOOLEAN'){
    value = param.getBooleanValue();
    var builder = Whirl.Builders.findBuilderByCode('chb_component');
    builder.setValue(value);
    
  } else if(type == 'DATE'){ 
    value = param.getDateValue();
    var builder = Whirl.Builders.findBuilderByCode('df_component');
    builder.setValue(value);
  } else if(type == 'LIST'){ 
    value = param.getListValue();
    var builder = Whirl.Builders.findBuilderByCode('mcb_component');
    builder.setFieldValue(value);
