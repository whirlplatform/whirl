{
  "source": "./generated-sources/jsdoc",
  "destination": "./docs",
  "includes": [
    "\\.es6$"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": {
          "enable": true
        },
        "unexportedIdentifier": {
          "enable": true
        },
        "typeInference": {
          "enable": true
        },
        "brand": {
          "title": "Whirl Platform JavaScript API"
        }
      }
    }
  ]
}

